package(default_visibility = ["//visibility:public"])

licenses(["notice"])

load(
    "@io_bazel_rules_go//go:def.bzl",
    "go_library",
    "go_test",
)

go_test(
    name = "go_default_test",
    srcs = ["tagger_test.go"],
    embed = [":go_default_library"],
    deps = [

    ],
)

go_library(
    name = "go_default_library",
    srcs = ["tagger.go"],
    importpath = "k8s.io/test-infra/prow/custom-reporter/guest-test-infra/tagutil",
    deps = [
      "//prow/custom-reporter/guest-test-infra/version:go_default_library"
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
)
