### job-env
readonly version_old='1.3'
readonly version_new='latest'
readonly version_infix='c1-3-glat'
readonly image_old='container_vm'
readonly image_new='gci'


E2E_OPT="--check_version_skew=false"
E2E_UPGRADE_TEST="true"
GINKGO_UPGRADE_TEST_ARGS="--ginkgo.focus=\[Feature:ClusterUpgrade\] --upgrade-target=ci/latest-${version_new} --upgrade-image=${image_new}"
JENKINS_PUBLISHED_SKEW_VERSION="ci/latest-${version_new}"
JENKINS_PUBLISHED_VERSION="ci/latest-${version_old}"
[ "${version_new}" = "latest" ] && JENKINS_PUBLISHED_SKEW_VERSION="ci/latest"
[ "${version_old}" = "latest" ] && JENKINS_PUBLISHED_VERSION="ci/latest"
JENKINS_USE_SKEW_TESTS="true"
KUBE_GKE_IMAGE_TYPE="${image_old}"
PROJECT="gke-up-c1-3-glat-up-clu-n"
ZONE="us-central1-c"

