### job-env
E2E_NAME="e2e-enormous-cluster"
GINKGO_TEST_ARGS="--ginkgo.focus=\[Feature:Performance\] \
    --kube-api-content-type=application/vnd.kubernetes.protobuf \
    --allowed-not-ready-nodes=20"
# TODO: re-enable service creation after we debug problems with this suite
CREATE_SERVICES="true"
CLUSTER_IP_RANGE="10.224.0.0/12"
NUM_NODES="3000"
ALLOWED_NOTREADY_NODES="20"
# Increase throughput in master components.
CONTROLLER_MANAGER_TEST_ARGS="--kube-api-qps=100 --kube-api-burst=100"
SCHEDULER_TEST_ARGS="--kube-api-qps=100 --kube-api-burst=100"
# Increase limit for inflight requests in apiserver.
APISERVER_TEST_ARGS="--max-requests-inflight=1000"
# Increase throughput in Load test.
LOAD_TEST_THROUGHPUT=30

