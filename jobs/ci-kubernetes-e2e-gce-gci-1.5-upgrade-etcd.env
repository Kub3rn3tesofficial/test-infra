### job-env

STORAGE_BACKEND=etcd2
# In 1.5 branch, in test configs we need to set:
# TEST_ETCD_VERSION & TEST_ETCD_IMAGE
# which are then assigned to ETCD_VERSION & ETCD_IMAGE.
TEST_ETCD_VERSION=2.2.1
TEST_ETCD_IMAGE=2.2.1
STORAGE_MEDIA_TYPE=application/json

E2E_OPT=--check_version_skew=false
E2E_UPGRADE_TEST=true
GINKGO_UPGRADE_TEST_ARGS=--ginkgo.focus=\[Feature:EtcdUpgrade\] --etcd-upgrade-storage=etcd3 --etcd-upgrade-version=3.0.17
KUBE_NODE_OS_DISTRIBUTION=gci
PROJECT=gce-up-g1-5-up-etcd


SKEW_KUBECTL=y
