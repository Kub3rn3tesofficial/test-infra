load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = ["escape.go"],
    importmap = "k8s.io/test-infra/vendor/gocloud.dev/internal/escape",
    importpath = "gocloud.dev/internal/escape",
    visibility = ["//vendor/gocloud.dev:__subpackages__"],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
