load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = [
        "build.go",
        "build_validation.go",
        "catalog.go",
        "catalog_validation.go",
        "core.go",
        "core_validation.go",
        "domain.go",
        "domain_validation.go",
        "error.go",
        "validation.go",
    ],
    importmap = "k8s.io/test-infra/vendor/github.com/traiana/okro/okro/api/v1beta2",
    importpath = "github.com/traiana/okro/okro/api/v1beta2",
    visibility = ["//visibility:public"],
    deps = [
        "//vendor/github.com/go-ozzo/ozzo-validation:go_default_library",
        "//vendor/github.com/traiana/okro/okro/pkg/util/errorx:go_default_library",
        "//vendor/github.com/traiana/okro/okro/pkg/util/hashx:go_default_library",
        "//vendor/k8s.io/apimachinery/pkg/apis/meta/v1/validation:go_default_library",
        "//vendor/k8s.io/apimachinery/pkg/util/validation/field:go_default_library",
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
