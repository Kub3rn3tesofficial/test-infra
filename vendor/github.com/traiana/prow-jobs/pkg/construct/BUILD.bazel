load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = [
        "catalog.go",
        "domain.go",
        "repo-pipelines.go",
    ],
    importmap = "k8s.io/test-infra/vendor/github.com/traiana/prow-jobs/pkg/construct",
    importpath = "github.com/traiana/prow-jobs/pkg/construct",
    visibility = ["//visibility:public"],
    deps = [
        "//vendor/github.com/traiana/okro/okro/api/ci/v1beta2:go_default_library",
        "//vendor/github.com/traiana/okro/okro/api/v1beta2:go_default_library",
        "//vendor/k8s.io/apimachinery/pkg/util/yaml:go_default_library",
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
